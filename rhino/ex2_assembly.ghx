from compas_ghpython import draw_frame


G_element = []
G_neighbors = []
G_course = []

key_index = assembly.network.key_index()
keys = [key_index[key] for key in assembly.network.nodes()]


if show_element:
    element = assembly.element(counter)
    artist = MeshArtist(element.mesh)
    mesh = artist.draw_mesh()
    G_element.append(mesh)

if show_neighbors:
    nbrs = [key_index[key] for key in assembly.network.neighbors(key=counter)]
    elements = [assembly.element(key) for key in nbrs]
    for element in elements:
        artist = MeshArtist(element.mesh)
        mesh = artist.draw_mesh()
        G_neighbors.append(mesh)

if show_course:
    course = assembly.network.node_attribute(key=counter, name='course')
    keys_in_course = list(assembly.network.nodes_where({'course': course}))
    elements = [assembly.element(key) for key in keys_in_course]
    for element in elements:
        artist = MeshArtist(element.mesh)
        mesh = artist.draw_mesh()
        G_course.append(mesh)

if show_connections:
    #elements = [assembly.element(key) for key in keys]
    #lines = assembly.network.to_lines() #does not work because edges are not transformed!
    G_connections = []
    edges = list(assembly.connections())
    for e in edges:
        p1 = assembly.element(e[0]).frame.point
        p2 = assembly.element(e[1]).frame.point
        G_connections += [rg.Line(p1[0],p1[1],p1[2],p2[0],p2[1],p2[2])]

if show_sequencing:
    from assembly_information_model.assembly.sequencing import assembly_building_sequence
    sequ = assembly_building_sequence(assembly, counter)
    print sequ 
