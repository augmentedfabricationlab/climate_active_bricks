                    <item name="CodeInput" type_name="gh_string" type_code="10">import Rhino.Geometry as rg
import math as m



def list_from_2darray(array):
    return [item for sublist in array for item in sublist]
    
def create_planegrid(brep, pt_base, pt_end, interval, bricklen):
    """ create diamond plane grid according to even and
    odd rows for a regualr stretcher bond.
    udiv: rows
    vdiv: cols (cols should be an odd number,
    for culling each second col to create even and odd courses)
    """

    curves = rg.Brep.CreateContourCurves(brep, pt_base, pt_end, interval)
    clen = curves[0].GetLength()
    cols = int(clen/(bricklen+0.020))*2
    print cols
    
    plane_grid = []

    for j, c in enumerate(curves):
        row = []
        vparams = rg.Curve.DivideByCount(c, cols, True)
        planes = rg.Curve.GetPerpendicularFrames(c, vparams)
        
        for i, plane in enumerate(planes):

            plane.Rotate(m.radians(90), plane.YAxis)
            #plane.Rotate(m.radians(-90), plane.ZAxis)
            #plane.Rotate(m.radians(180), plane.XAxis)

            if j%2 == 0 and i%2 == 0: #even course
                if i==0:
                    plane.Translate(plane.XAxis*-bricklen/4)
                    plane.Rotate(m.radians(90), plane.ZAxis)
                elif i==len(planes)-1:
                    plane.Translate(plane.XAxis*bricklen/4)
                    plane.Rotate(m.radians(90), plane.ZAxis)
                row.append(plane)
            if j%2 == 1 and i%2 == 1: #odd course

                row.append(plane)

        plane_grid.append(row)
    return plane_grid

plane_grid = create_planegrid(brep, pt_base, pt_end, interval, bricklen)
